<Window x:Class="PaintAnalog.Views.ToolsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tools" Height="225" Width="300">
    <StackPanel>
        <Button Content="Brush" Click="BrushSelected" Margin="10" />
        <Button Content="Eraser" Click="EraserSelected" Margin="10" />
        <Button Content="Fill" Click="FillSelected" Margin="10" />
        <Button Content="Selection" Click="SelectionSelected" Margin="10" />
    </StackPanel>
</Window>
